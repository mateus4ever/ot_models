{
  "general": {
    "verbose": false,
    "random_state": 42,
    "train_test_split": 0.7,
    "save_signals": false,
    "debug_mode": false
  },
  "data_loading": {
    "data_source": "data/eurusd",
    "max_records": 500000,
    "file_pattern": "*.csv"
  },
  "mathematical_operations": {
    "zero": 0,
    "unity": 1,
    "random_seed": null
  },
  "array_indexing": {
    "first_index": 0,
    "second_index": 1,
    "third_index": 2
  },
  "constants": {
    "percentile": 0.05,
    "forward_window": 240
  },
  "system_configuration": {
    "windows_compatibility": {
      "max_cores": 16
    },
    "logging": {
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "debug_configuration": {
    "enable_trade_debug": false,
    "enable_position_debug": false,
    "enable_metrics_debug": false,
    "enable_direct_math_check": false,
    "print_trade_details": false,
    "log_trades": false,
    "trade_debug_count": 0,
    "enable_fee_debug": false
  },
  "backtesting": {
    "print_trades": false,
    "verbose_output": false
  },
  "optimization": {
    "defaults": {
      "n_combinations": 50,
      "data_sample_size": 50000,
      "max_workers": 2,
      "quiet_mode": true
    },
    "bayesian_defaults": {
      "n_calls": 50,
      "initial_points_divisor": 5,
      "acquisition_function": "LCB"
    },
    "bayesian": {
      "n_calls": 50,
      "n_initial_points": 10,
      "acquisition_function": "LCB",
      "acq_optimizer": "auto"
    },
    "parameter_ranges": {
      "stop_loss_min": 0.005,
      "stop_loss_max": 0.050,
      "take_profit_min": 0.015,
      "take_profit_max": 0.080,
      "max_position_min": 0.2,
      "max_position_max": 1.0,
      "price_vs_ma_min": 0.0005,
      "price_vs_ma_max": 0.005,
      "momentum_min": 0.001,
      "momentum_max": 0.01,
      "volatility_ratio_min": 1.2,
      "volatility_ratio_max": 2.5,
      "kama_period_min": 10,
      "kama_period_max": 30,
      "kalman_noise_min": 0.0001,
      "kalman_noise_max": 0.01,
      "signal_weight_min": 0.3,
      "signal_weight_max": 0.8
    },
    "quiet_overrides": {
      "general": {
        "verbose": false
      },
      "debug_configuration": {
        "trade_debug_count": 0,
        "enable_fee_debug": false,
        "enable_trade_debug": false,
        "enable_position_debug": false,
        "enable_metrics_debug": false,
        "print_trade_details": false,
        "log_trades": false
      },
      "backtesting": {
        "print_trades": false,
        "verbose_output": false
      }
    },
    "progress": {
      "report_interval": 5
    },
    "fitness": {
      "penalties": {
        "severe_penalty_value": -999.0
      },
      "requirements": {
        "min_trades": 10,
        "max_drawdown_limit": 0.5
      },
      "weights": {
        "return_component": 100.0,
        "sharpe_component": 10.0,
        "drawdown_component": 50.0
      }
    },
    "analysis": {
      "top_performers_percentile": 0.1
    },
    "error_messages": {
      "no_valid_results": "No valid optimization results found"
    },
    "display": {
      "separator_char": "=",
      "separator_length": 120,
      "dash_char": "-",
      "top_display_count": 10,
      "error_template": "ERROR: {error}",
      "no_results_message": "No valid results to display",
      "results_title": "OPTIMIZATION RESULTS - TOP PARAMETER COMBINATIONS",
      "header_format": "{rank:<4} {stop_loss:<10} {take_profit:<12} {max_pos:<8} {sharpe:<8} {trades:<7} {return:<8} {fitness:<8}",
      "column_labels": {
        "rank": "Rank",
        "stop_loss": "Stop Loss",
        "take_profit": "Take Profit",
        "max_pos": "Max Pos",
        "sharpe": "Sharpe",
        "trades": "Trades",
        "return": "Return",
        "fitness": "Fitness"
      },
      "row_format": "{rank:<4} {stop_loss:.1f}% {take_profit:.1f}% {max_pos:.0f}% {sharpe:.2f} {trades:<7} {return:.1f}% {fitness:.1f}",
      "summary_title": "BEST COMBINATION SUMMARY:",
      "summary_format": "Return: {return:.2f}% | Sharpe: {sharpe:.2f} | Trades: {trades}\nStop Loss: {stop_loss:.1f}% | Take Profit: {take_profit:.1f}%\nMax Position: {max_position:.0f}% | Fitness Score: {fitness:.1f}"
    },
    "messages": {
      "optimization_title": "FAST PARAMETER OPTIMIZATION",
      "testing_combinations": "Testing {n_combinations} random parameter combinations",
      "quiet_mode": "Quiet mode: {quiet}",
      "parallel_workers": "Parallel workers: {max_workers}",
      "setup_message": "Loading data and training strategy (one-time setup)...",
      "data_loaded": "✓ Data loaded: {records} records",
      "strategy_trained": "✓ Strategy trained in {time:.1f} seconds",
      "optimization_running": "Running parameter optimization...",
      "optimization_failed": "Optimization failed: {error}",
      "optimization_completed": "\nOptimization completed in {duration:.1f} seconds\nValid results: {valid}/{total}"
    },
    "robustness": {
      "cv_threshold_robust": 0.3,
      "cv_threshold_sensitive": 0.6,
      "top_performers_percentile": 0.2,
      "stability_window_size": 10,
      "moderate_std_multiplier": 1.5,
      "fitness_percentiles": [
        0.25,
        0.5,
        0.75,
        0.9,
        0.95
      ],
      "plateau_threshold": 0.1,
      "strong_correlation_threshold": 0.7,
      "moderate_correlation_threshold": 0.4,
      "high_robustness_threshold": 0.7,
      "moderate_robustness_threshold": 0.5
    }
  },
  "hybrid_strategy": {
    "ml_components": {
      "regime_detection": "rule_based",
      "volatility_prediction": "enabled",
      "trend_duration": "disabled"
    },
    "technical_indicators": {
      "ma_period": 20,
      "rsi_period": 14,
      "bollinger_period": 20,
      "bollinger_std": 2
    },
    "signal_generation": {
      "ml_weight": 0.6,
      "technical_weight": 0.4,
      "confidence_threshold": 0.5
    }
  },
  "ml_models": {
    "regime_detection": {
      "model_type": "rule_based",
      "lookback_period": 50,
      "volatility_threshold": 0.02,
      "trend_threshold": 0.001
    },
    "volatility_prediction": {
      "model_type": "random_forest",
      "n_estimators": 100,
      "max_depth": 10,
      "random_state": 42,
      "features": [
        "returns",
        "volatility",
        "volume",
        "rsi",
        "bollinger_position"
      ]
    }
  },
  "feature_engineering": {
    "technical_features": {
      "price_features": [
        "sma_5",
        "sma_20",
        "ema_12",
        "ema_26"
      ],
      "momentum_features": [
        "rsi_14",
        "macd",
        "momentum_10"
      ],
      "volatility_features": [
        "atr_14",
        "bollinger_width",
        "volatility_ratio"
      ]
    },
    "ml_features": {
      "regime_features": [
        "regime_strength",
        "regime_direction",
        "regime_duration"
      ],
      "prediction_features": [
        "volatility_forecast",
        "trend_probability"
      ]
    }
  },
  "risk_management": {
    "stop_loss_pct": 0.02,
    "take_profit_pct": 0.04,
    "max_position_size": 0.5,
    "max_daily_trades": 10,
    "risk_per_trade": 0.02
  },
  "transaction_costs": {
    "commission_pct": 0.0001,
    "slippage_pct": 0.00005,
    "spread_pct": 0.00002
  },
  "performance_metrics": {
    "benchmark": "buy_and_hold",
    "risk_free_rate": 0.02,
    "confidence_level": 0.95
  }
}